         Cells="Group,Select,Empty,Rows,Columns,Print,ExpandAll,CollapseAll"
         GroupType="Bool" GroupLabelRight="Group by section" GroupOnChange="GroupOn OR GroupOff" GroupFormula="Grid.Grouped" GroupCanEdit="1"
