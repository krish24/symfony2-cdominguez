   <Cols>
      <C Name="I" Type="Int"/>
      <C Name="N" Width="300" GroupWidth="420" Class="Name" WhiteChars=" "/>
      <C Name="S" RawSort="2"
         Type="Enum" EnumKeys="|EGrid|EGantt|Cells|Tree|Sort|Filter|Drag|Paging|MasterDetail|Gantt" 
         Enum="|Full examples - Grid|Full examples - Gantt|Cells - types, formats, editing, buttons, span, custom controls|Tree - rows, column and grouping|Sorting rows|Filter and Search|Dragging rows, cells and Gantt objects|Paging rows, child rows and columns|Master and detail tables and trees|Gantt and Run charts"
         />
      <C Name="D"/>
   </Cols>
   <Header N="Name" S="Section" D="Description" I=""/>
   <Panel Visible="0" CanHide="0"/>
   <Pager Visible="0" CanHide="0"/>
