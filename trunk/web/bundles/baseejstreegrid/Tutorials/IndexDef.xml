   <Head>
      <Filter Def="R" FocusCell="Border" id="Filter" CanFocus="1" ICanEdit="0"
         NEmptyValue="&lt;i>Enter text to filter by&lt;/i>" NClass="" NOnDown="ShowFilterMenuF"
         />
