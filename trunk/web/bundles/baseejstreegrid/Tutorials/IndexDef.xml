<Grid>
   <Cfg id="Examples" SuppressCfg="0" ScrollTopLap="0" SearchLap="1" SearchCaseSensitive="1" Editing="0" Adding="0" Selecting="1" Deleting="0" Moving="0" SaveAttrsTrim="255"
