            FLOW="1/3/2010~1/5/2010;1/7/2010~1/9/2010"
            TIP="Flow with not editable main bar"
            />
         <I id="4"
            FLOW="1/3/2010~1/5/2010;1/7/2010~1/9/2010;1/11/2010"
            TIP="Flow without main bar"
            />
         <I id="5" 
            FLOW="1/8/2010~1/9/2010;1/11/2010~1/14/2010"
