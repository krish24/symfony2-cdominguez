<Grid>
   <Cfg id="Flow" NoVScroll="1" IdChars="0123456789" NumberId="1"/>
   <Cfg DefaultDate="1/1/2010"/>
   <LeftCols>
      <C Name="id" Type="Int" CanEdit="0" CanSort="0"/>
      <C Name="START" Type="Date" Format="dddddd" EditFormat="%d"/>
      <C Name="END" Type="Date" Format="dddddd" EditFormat="%d"/>
      <C Name="FLOW" Type="Date" Range="1" Width="220" Format="dddddd" EditFormat="%d"/>
      <C Name="DES" Type="Text" Visible="0"/>
      <C Name="TIP" Type="Text" Width="170"/>
   </LeftCols>
   <Cols>
      <C Name="GANTT" Type="Gantt" 
         GanttDataUnits="d" GanttUnits="d" GanttWidth="16"
         GanttFlow="FLOW" GanttFlowText="TIP" GanttStart="START" GanttEnd="END" GanttDescendants="DES"
         GanttEndLast="1" GanttLeft="1" GanttRight="1"
         GanttHeader1="d#%d"
         GanttFlowTip="*id*. *TIP* (*COMP*%)&lt;br>*Start* - *End* (*Duration*)"
         GanttObject="Main,Flow"
         />
   </Cols>
   <Body>
      <B>
         <I id="0"/>
         <I id="1" 
            START="1/1/2010" END="1/8/2010"
            FLOW="1/1/2010~1/8/2010" 
